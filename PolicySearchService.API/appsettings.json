{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "PolicySearchService"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "PolicySearchService",
      "port": "5060",
      "hostName": "localhost"
    }
  },
  "ConnectionStrings": {
    "ElasticSearchConnection": "THIS IS SECRET"
  },
  "RabbitMQ": {
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "Port": 5672,
    "Hostname": "localhost",
    "RequestTimeout": "00:00:10",
    "PublishConfirmTimeout": "00:00:01",
    "RecoveryInterval": "00:00:01",
    "PersistentDeliveryMode": true,
    "AutoCloseConnection": true,
    "AutomaticRecovery": true,
    "TopologyRecovery": true,
    "Exchange": {
      "Name": "microservices-poc",
      "Durable": true,
      "AutoDelete": false,
      "Type": "Topic"
    },
    "Queue": {
      "Prefix": "payments-service-",
      "AutoDelete": false,
      "Durable": true,
      "Exclusive": false
    }
  }
}